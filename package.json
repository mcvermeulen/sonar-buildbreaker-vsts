{
  "name": "sonar-buildbreaker-vsts",
  "version": "1.0.0",
  "description": "These tasks help automate bumping version numbers and changing package names at build time.",
  "main": "index.js",
  "scripts": {
    "postinstall": "cd extensions/sonarqube/tasks/buildbreaker && npm install",
    "build": "tsc -p extensions/sonarqube/tasks/buildbreaker",
    "pack": "tfx extension create --manifest-globs extensions/sonarqube/vss-extension.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simondel/sonar-buildbreaker-vsts.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/simondel/sonar-buildbreaker-vsts/issues"
  },
  "homepage": "https://github.com/simondel/sonar-buildbreaker-vsts#readme",
  "devDependencies": {
    "@types/node": "^16.11.39",
    "tfx-cli": "0.17.0",
    "typescript": "5.5.4"
  }
}
